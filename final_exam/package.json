    "foldero": "~0.1.1",
    "glob": "~6.0.4",
    "gulp": "~3.9.0",
    "gulp-changed": "~1.3.0",
    "gulp-concat": "^2.6.0",
    "gulp-cssnano": "~2.1.0",
    "gulp-data": "~1.2.1",
    "gulp-htmlmin": "~1.3.0",
    "gulp-if": "~2.0.0",
    "gulp-imagemin": "~2.4.0",
    "gulp-load-plugins": "~1.2.0",
    "gulp-newer": "~1.1.0",
    "gulp-npm-files": "^0.1.3",
    "gulp-plumber": "~1.0.1",
    "gulp-postcss": "~6.0.1",
    "gulp-rename": "~1.2.2",
    "gulp-sass": "~2.1.1",
